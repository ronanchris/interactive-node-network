{
  "name": "interactive-node-network",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "postcss.config.js",
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.21.10",
    "caniuse-lite": "^1.0.30001519",
    "convert-source-map": "^2.0.0",
    "debug": "^4.3.4",
    "electron-to-chromium": "^1.4.477",
    "escalade": "^3.1.1",
    "express": "^4.21.2",
    "gensync": "^1.0.0-beta.2",
    "globals": "^11.12.0",
    "js-tokens": "^4.0.0",
    "jsesc": "^3.0.2",
    "json5": "^2.2.3",
    "lru-cache": "^5.1.1",
    "ms": "^2.1.3",
    "nanoid": "^3.3.6",
    "node-releases": "^2.0.13",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "rollup": "^3.26.3",
    "semver": "^6.3.1",
    "source-map-js": "^1.0.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "update-browserslist-db": "^1.0.11",
    "vite": "^6.2.2",
    "yallist": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "gh-pages": "^6.3.0",
    "serve": "^14.2.4",
    "terser": "^5.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "overrides": {
    "vite": {
      "esbuild": "^0.25.1"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-diagrams": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' scripts/docs/diagrams.ts",
    "check-env": "ts-node scripts/check-environment.ts",
    "docs:check": "ts-node scripts/docs/automation.ts check",
    "docs:coverage": "node --loader ts-node/esm scripts/docs/coverage.ts",
    "docs:update": "npm run docs:check && npm run generate-diagrams",
    "docs:learn": "ts-node scripts/docs/automation.ts learn",
    "docs:session": "ts-node scripts/docs/automation.ts session",
    "setup-machine": "ts-node scripts/setup-machine.ts",
    "verify-machine": "ts-node scripts/setup-machine.ts verify",
    "test:machine-detection": "ts-node scripts/tests/test-machine-detection.ts",
    "init:machine-management": "ts-node scripts/init-machine-management.ts",
    "fix-links": "ts-node scripts/fix-links.ts",
    "docs:check-links": "node --loader ts-node/esm scripts/docs/link-checker.ts",
    "docs:fix-links": "node --loader ts-node/esm scripts/docs/link-fixer.ts",
    "docs:reorganize": "node --loader ts-node/esm scripts/docs/reorganize.ts",
    "docs:health": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/docs/generate-health-report.ts",
    "docs:health:watch": "nodemon --watch docs --ext md --exec 'npm run docs:health'",
    "docs:dashboard": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/docs/serve-dashboard.ts",
    "docs:update-timestamps": "ts-node scripts/docs/update-timestamps.ts",
    "docs:generate-changelog": "ts-node scripts/docs/generate-changelog.ts",
    "docs:check-coverage": "ts-node scripts/docs/coverage.ts",
    "docs:maintain": "npm run docs:health && npm run docs:update-timestamps && npm run docs:check-coverage",
    "build:health": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node scripts/docs/generate-health-report.ts",
    "build:app": "tsc && vite build --outDir dist/app",
    "build:status": "mkdir -p dist/status && cp -r docs/status/* dist/status/",
    "predeploy": "npm run build && npm run build:status",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
